# Application
NODE_ENV=development
PORT=3000

# Database (for local development without Docker)
DATABASE_URL=postgresql://username:password@localhost:5432/claude_template_dev
# Database (for Docker - this is automatically set in docker-compose.yml)
# DATABASE_URL=postgresql://postgres:postgres123@postgres:5432/claude_template_dev
DATABASE_LOGGING=true

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this
BCRYPT_ROUNDS=10

# API Documentation
SWAGGER_ENABLED=true

# Logging
LOG_LEVEL=debug